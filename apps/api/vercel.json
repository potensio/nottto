{
  "buildCommand": "pnpm run build",
  "installCommand": "cd ../.. && pnpm install",
  "outputDirectory": "api",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/api",
      "permanent": false
    }
  ]
}
